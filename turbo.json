{
  "tasks": {
    "build": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "^dev"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "tsc": {
      "dependsOn": [
        "^tsc"
      ]
    }
  }
}